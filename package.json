{
  "name": "<%= name %>",
  "version": "<%= version %>",
  "private": "<%= private %>",
  "author": "<%= author %>",
  "description": "<%= description %>",
  "license": "<%= license %>",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_EVN=development vite",
    "build": "cross-env NODE_EVN=production vite build",
    "build:check": "cross-env NODE_EVN=production vuedx-typecheck . && vite build",
    "preview": "vite preview",
    "build:preview": "npm run build && npm run preview",
    "typecheck": "vuedx-typecheck .",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:ls-lint": "ls-lint"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.0.1",
    "autoprefixer": "^10.2.4",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "vue": "^3.0.7",
    "vue-router": "^4.0.4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.8",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@ls-lint/ls-lint": "^1.9.2",
    "@types/lodash-es": "^4.17.4",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^14.14.31",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@vitejs/plugin-legacy": "^1.3.1",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vitejs/plugin-vue-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.6",
    "@vuedx/typecheck": "^0.6.3",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "husky": "^5.1.2",
    "less": "^4.1.1",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.2.0",
    "vite": "^2.0.4",
    "vite-plugin-mock": "^2.1.5",
    "vite-plugin-style-import": "^0.7.5"
  },
  "engines": {
    "node": "^12 || ^14 || ^15 || ^16"
  }
}
